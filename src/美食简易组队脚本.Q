Event Form_team_config.Button_new_config.Click
    Form_set_new_team_config.Show
End Event

Event Form_single_config.Button_new_config.Click
    Form_set_new_single_config.Show
End Event

Event Form_team_config.Button_delete_config.Click
    Section = Split(Form_team_config.ComboBox_config_select.List, "|")(Form_team_config.ComboBox_config_select.ListIndex)
    FilePath = Form1.BrowseBox_team_ini_path.Path
    Call Lib.ini文件操作.DeleteIni(Section, "", FilePath)
    MessageBox "删除操作完成，请重新加载配置文件"
    Form_team_config.Close
End Event

Event Form_single_config.Button_delete_config.Click
    Section = Split(Form_single_config.ComboBox_config_select.List, "|")(Form_single_config.ComboBox_config_select.ListIndex)
    FilePath = Form1.BrowseBox_single_ini_path.Path
    Call Lib.ini文件操作.DeleteIni(Section, "", FilePath)
    MessageBox "删除操作完成，请重新加载配置文件"
    Form_single_config.Close
End Event

Event Form_team_config.UnLoad
    Call Form_set_new_team_config.Close()
End Event

Event Form_single_config.UnLoad
    Call Form_set_new_single_config.Close()
End Event

Event Form_set_new_team_config.Button_new_config_ok.Click
    Section = Form_set_new_team_config.InputBox_new_config_name.Text
    FilePath = Form1.BrowseBox_team_ini_path.Path
    Call Lib.ini文件操作.WriteIni(Section, "描述", Form_set_new_team_config.InputBox_new_config_describe.Text, FilePath)
    Call 写入双人配置(Section, FilePath)
    MessageBox "新建操作完成，请重新加载配置文件"
    Form_team_config.Close
End Event

Event Form_set_new_single_config.Button_new_config_ok.Click
    Section = Form_set_new_single_config.InputBox_new_config_name.Text
    FilePath = Form1.BrowseBox_single_ini_path.Path
    Call Lib.ini文件操作.WriteIni(Section, "描述", Form_set_new_single_config.InputBox_new_config_describe.Text, FilePath)
    Call 写入单人配置(Section, FilePath)
    MessageBox "新建操作完成，请重新加载配置文件"
    Form_single_config.Close
End Event

Event Form_set_new_team_config.Button_new_config_cancel.Click
    Call Form_set_new_team_config.Close()
End Event

Event Form_set_new_single_config.Button_new_config_cancel.Click
    Call Form_set_new_single_config.Close()
End Event

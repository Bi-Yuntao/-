    Delay 1000
    Call Plugin.Bkgnd.KeyDown(handle_1p, 96)
    Delay 50
    Call Plugin.Bkgnd.KeyDown(handle_1p, 96)
    Delay 1000
    Call Plugin.Bkgnd.LeftClick(handle_1p, (跨服创房密码x坐标(关卡难度序号 - 1) + 50) * 缩放倍率, (跨服创房密码y坐标(关卡难度序号 - 1) + 40) * 缩放倍率)//点击创建房间
    Delay 1000
    Call Lib.美食大战老鼠.房间内选择卡片组(handle_1p, CInt(Form1.InputBox_tab7_deck_id.Text), 缩放倍率)//1P选择卡片组
    Delay 200
	
    If Form1.CheckBox_ban_2p.Value = 0 Then
        Call 追加信息输出框("2P正在进入房间")
        Call Plugin.Bkgnd.LeftClick(handle_2p, 620 * 缩放倍率, 80 * 缩放倍率)//打开关卡类型下拉框
        Delay 500
        Call Plugin.Bkgnd.LeftClick(handle_2p, 630 * 缩放倍率, 关卡类型组合框y坐标(Form1.ComboBox_tab7_level_type.ListIndex) * 缩放倍率)//选择关卡类型
        Delay 1000
		
        search_1p_room = True
        search_limit = 0
        While search_1p_room
            XY = Plugin.Bkgnd.FindPic(handle_2p, 400, 100, 925, 470, Form1.BrowseBox_tab7_1p_name_path.Path, 0, 0.9)
            If Split(XY, "|")(0) > 0 And Split(XY, "|")(1) > 0 Then 
                Call Plugin.Bkgnd.LeftClick(handle_2p, Split(XY, "|")(0) * 缩放倍率, Split(XY, "|")(1) * 缩放倍率)//点击房间
                Delay 500
                Call Plugin.Bkgnd.LeftClick(handle_2p, Split(XY, "|")(0) * 缩放倍率, Split(XY, "|")(1) * 缩放倍率)//点击房间
                Delay 1000
                Call Plugin.Bkgnd.LeftClick(handle_2p, 480 * 缩放倍率, 300 * 缩放倍率)//点击密码输入框
                Delay 1000
                Call Plugin.Bkgnd.KeyDown(handle_2p, 96)
                Delay 50
                Call Plugin.Bkgnd.KeyDown(handle_2p, 96)
                Delay 1000
                Call Plugin.Bkgnd.LeftClick(handle_2p, 490 * 缩放倍率, 360 * 缩放倍率)//进入房间
                Delay 1000
                Call Lib.美食大战老鼠.房间内选择卡片组(handle_2p, CInt(Form1.InputBox_tab7_deck_id.Text), 缩放倍率)//2P选择卡片组
                Delay 200
                search_1p_room = False
            Else
                XY2 = Plugin.Bkgnd.FindColorEx(handle_2p, 812, 424, 837, 453, "512C00", 0, 1)
                If Split(XY2, "|")(0) > 0 And Split(XY2, "|")(1) > 0 Then 
                    MessageBox "未找到1P房间"//已经是最后一页
                    Exit Sub
                Else 
                    If search_limit >= 15 Then //翻了15页仍未找到则退出
                        search_1p_room = False
                    End If
                    Call Plugin.Bkgnd.LeftClick(handle_2p, 699 * 缩放倍率, 488 * 缩放倍率)//点击下一页
                    Delay 500
                    search_limit = search_limit + 1
                End If
            End If
        Wend
    End If
	
    Call 追加信息输出框("应用组队配置方案")
	
    Section = Form1.InputBox_tab7_config_name.Text
    If Form1.CheckBox_ban_2p.Value = 1 Then 
        FilePath = Form1.BrowseBox_single_ini_path.Path
        Call 应用单人配置(Section, FilePath)
		
        For i = 1 To CInt(Form1.InputBox_tab7_circle_num.Text)
            追加信息输出框 ("开始第" & i & "局")
            Call 单人从准备到翻牌(0, 0, Form1.InputBox_tab7_card_position.Text)
        Next
    Else 
        FilePath = Form1.BrowseBox_team_ini_path.Path
        Call 应用组队配置(Section, FilePath)
		
        For i = 1 To CInt(Form1.InputBox_tab7_circle_num.Text)
            追加信息输出框 ("开始第" & i & "局")
            Call 组队从准备到翻牌(0, 0, Form1.InputBox_tab7_card_position.Text)
        Next
    End If
	
    Call Plugin.Bkgnd.LeftClick(handle_1P, 920 * 缩放倍率, 560 * 缩放倍率)//退出房间
    Call Plugin.Bkgnd.LeftClick(handle_1P, 915 * 缩放倍率, 30 * 缩放倍率)//退出跨服
    Call Plugin.Bkgnd.LeftClick(handle_2P, 920 * 缩放倍率, 560 * 缩放倍率)//退出房间
    Call Plugin.Bkgnd.LeftClick(handle_2P, 915 * 缩放倍率, 30 * 缩放倍率)//退出跨服
	
    If 启用一键日常模式 Then 
        Exit Sub
    End If
    ThreadID_now = 0
    TracePrint ThreadID_now
    追加信息输出框("完成跨服")
    If Form1.CheckBox_tab7_finish_remind.Value = 1 Then 
        MessageBox "完成跨服"
    End If
End Sub


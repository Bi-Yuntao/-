    If Form1.CheckBox_ban_2p.Value = 0 Then
        invite_result = Lib.美食大战老鼠.组队好友邀请(handle_1p, handle_2p, Form1.BrowseBox_tab4_2p_name_path.Path, 缩放倍率)

    Call Plugin.Bkgnd.LeftClick(handle_1p, 114 * 缩放倍率, 66 * 缩放倍率)//选择双人
    Delay 200
	
    tower_level = CInt(Form1.InputBox_tab8_tower_level.Text) //关卡层级
    right_click_num = CInt((tower_level - 1) / 15)  //向右次数
    down_to_up_num = ((tower_level - 1) Mod 15) + 1  //从下往上层数
    

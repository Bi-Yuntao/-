Function 写入双人配置(Section, FilePath)
    Call Lib.ini文件操作.WriteIni(Section, "1P放置位置", Form1.InputBox_1p_position.Text, FilePath)
    Call Lib.ini文件操作.WriteIni(Section, "2P放置位置", Form1.InputBox_2p_position.Text, FilePath)
	
    'Form_team_config.ProgressBar_update_config.Value = 2
	
    If Form1.CheckBox_1p_card1.Value = 1 Then //更新1P
        Call 写入1P配置(Section, FilePath, 1, 2)
    Else 
        Call Lib.ini文件操作.WriteIni(Section, "1P卡1", "0", FilePath)
    End If
	
    TracePrint "完成1P卡一"
    'Form_team_config.ProgressBar_update_config.Value = 10
	
    If Form1.CheckBox_1p_card2.Value = 1 Then 
        Call 写入1P配置(Section, FilePath, 2, 2)
    Else 
        Call Lib.ini文件操作.WriteIni(Section, "1P卡2", "0", FilePath)
    End If
	
    TracePrint "完成1P卡二"
    'Form_team_config.ProgressBar_update_config.Value = 20
	
    If Form1.CheckBox_1p_card3.Value = 1 Then 
        Call 写入1P配置(Section, FilePath, 3, 2)
    Else 
        Call Lib.ini文件操作.WriteIni(Section, "1P卡3", "0", FilePath)
    End If
	
    TracePrint "完成1P卡三"
    'Form_team_config.ProgressBar_update_config.Value = 30
	
    If Form1.CheckBox_1p_card4.Value = 1 Then 
        Call 写入1P配置(Section, FilePath, 4, 2)
    Else 
        Call Lib.ini文件操作.WriteIni(Section, "1P卡4", "0", FilePath)
    End If
	
    'Form_team_config.ProgressBar_update_config.Value = 40
	
    If Form1.CheckBox_1p_card5.Value = 1 Then 
        Call 写入1P配置(Section, FilePath, 5, 2)
    Else 
        Call Lib.ini文件操作.WriteIni(Section, "1P卡5", "0", FilePath)
    End If
	
    'Form_team_config.ProgressBar_update_config.Value = 50
	
    If Form1.CheckBox_2p_card1.Value = 1 Then  //更新2P
        Call 写入2P配置(Section, FilePath, 1)
    Else 
        Call Lib.ini文件操作.WriteIni(Section, "2P卡1", "0", FilePath)
    End If
	
    'Form_team_config.ProgressBar_update_config.Value = 60
	
    If Form1.CheckBox_2p_card2.Value = 1 Then 
        Call 写入2P配置(Section, FilePath, 2)
    Else 
        Call Lib.ini文件操作.WriteIni(Section, "2P卡2", "0", FilePath)
    End If
	
    'Form_team_config.ProgressBar_update_config.Value = 70
	
    If Form1.CheckBox_2p_card3.Value = 1 Then 
        Call 写入2P配置(Section, FilePath, 3)
    Else 
        Call Lib.ini文件操作.WriteIni(Section, "2P卡3", "0", FilePath)
    End If
	
    'Form_team_config.ProgressBar_update_config.Value = 80
	
    If Form1.CheckBox_2p_card4.Value = 1 Then 
        Call 写入2P配置(Section, FilePath, 4)
    Else 
        Call Lib.ini文件操作.WriteIni(Section, "2P卡4", "0", FilePath)
    End If
	
    'Form_team_config.ProgressBar_update_config.Value = 90
	
    If Form1.CheckBox_2p_card5.Value = 1 Then 
        Call 写入2P配置(Section, FilePath, 5)
    Else 
        Call Lib.ini文件操作.WriteIni(Section, "2P卡5", "0", FilePath)
    End If
	
    'Form_team_config.ProgressBar_update_config.Value = 100
    'Delay 500
    'Form_team_config.ProgressBar_update_config.Value = 0
End Function


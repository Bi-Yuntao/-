Function 应用组队配置(Section, FilePath)
    Form1.InputBox_1p_position.Text = Lib.ini文件操作.ReadIni(Section, "1P放置位置", FilePath)
    Form1.InputBox_2p_position.Text = Lib.ini文件操作.ReadIni(Section, "2P放置位置", FilePath)
	
    Form1.CheckBox_1p_card1.Value = 0
    Form1.CheckBox_1p_card2.Value = 0
    Form1.CheckBox_1p_card3.Value = 0
    Form1.CheckBox_1p_card4.Value = 0
    Form1.CheckBox_1p_card5.Value = 0
    Form1.CheckBox_2p_card1.Value = 0
    Form1.CheckBox_2p_card2.Value = 0
    Form1.CheckBox_2p_card3.Value = 0
    Form1.CheckBox_2p_card4.Value = 0
    Form1.CheckBox_2p_card5.Value = 0
	
    i = 1
    For 5
        TracePrint "1P卡" & i & "的值：" & Lib.ini文件操作.ReadIni(Section, "1P卡" & i, FilePath)
        If CInt(Lib.ini文件操作.ReadIni(Section, "1P卡" & i, FilePath)) = 1 Then 
            Call 应用1P配置(Section, FilePath, i, 2)
        End If
        TracePrint "2P卡" & i & "的值：" & Lib.ini文件操作.ReadIni(Section, "2P卡" & i, FilePath)
        If CInt(Lib.ini文件操作.ReadIni(Section, "2P卡" & i, FilePath)) = 1 Then 
            Call 应用2P配置(Section, FilePath, i)
        End If
        i = i + 1
    Next
	
End Function


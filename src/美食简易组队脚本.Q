Sub 关闭所有放卡线程
    If ThreadID_team_1p_card_bus <> 0 Then
        StopThread ThreadID_team_1p_card_bus
        ThreadID_team_1p_card_bus = 0
    End If
    If ThreadID_team_2p_card_bus <> 0 Then
        StopThread ThreadID_team_2p_card_bus
        ThreadID_team_2p_card_bus = 0
    End If
	
    '同时关闭1p放置卡片的线程
    If ThreadID_1p_card1 <> 0 Then 
        StopThread ThreadID_1p_card1

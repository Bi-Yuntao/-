    Delay 1000
    Call Plugin.Bkgnd.LeftClick(handle_1P, 920 * 缩放倍率, 560 * 缩放倍率)//退出房间
    Call Plugin.Bkgnd.LeftClick(handle_2P, 920 * 缩放倍率, 560 * 缩放倍率)//退出房间
	

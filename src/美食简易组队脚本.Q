Event Form1.Button_tab1_start_job1.Click
    If ThreadID_now <> 0 Then
        MessageBox "当前还有脚本正在执行"
    Else 
        Call 清除信息输出框()
		
        缩放倍率 = CInt(Form1.InputBox_scaling.Text)/100
		
        If Form1.CheckBox_ban_2p.Value = 1 Then 
            'handle_1p = 单人获取句柄(Int(Form1.ComboBox_1p.ListIndex))
            handle_1p = CLng(Form1.InputBox_1p_handle.Text)
            If handle_1p = 0 Then 
                MessageBox "未获取1P窗口句柄"
            Else 
                ThreadID_now = BeginThread(开房单人)
                TracePrint ThreadID_now
                追加信息输出框 ("启动脚本1")
            End If
        Else 
            handle_1p = CLng(Form1.InputBox_1p_handle.Text)
            handle_2p = CLng(Form1.InputBox_2p_handle.Text)

            是否继续挑战 = 1
        Else
            是否继续挑战 = 0
        End If
		
    Case "浮空岛"
        Call Lib.美食大战老鼠.切换地图(handle_1p, 待执行任务(0), 缩放倍率)
        Call Lib.美食大战老鼠.切换地图(handle_2p, 待执行任务(0), 缩放倍率)
		
        Call Lib.美食大战老鼠.浮空岛选择关卡(handle_1p, 待执行任务(1), 缩放倍率)
		
        是否有二阶段 = 1

        Delay 100
        
        TracePrint "往右" & right_click_num & "次"
        right_num = right_click_num
        While right_num > 0
            Call Plugin.Bkgnd.LeftClick(handle_1p, 149 * 缩放倍率, 576 * 缩放倍率)//往右
            Delay 100
            right_num = right_num - 1
        Wend
		
        TracePrint "选从下往上第" & down_to_up_num & "关"
        TracePrint 542 - 30 * (down_to_up_num - 1)

Function 单人获取句柄(way_1p)
    Dim hwnd
	
    TracePrint "登陆方式为：" & way_1p
    Select Case way_1p
    Case 0
        hwnd = Lib.美食大战老鼠.获取360句柄()
    Case 1
        hwnd = Lib.美食大战老鼠.获取微端句柄()
    Case 2
        hwnd = Lib.美食大战老鼠.获取枫叶内置flash句柄()
        /*Case 3
        hwnd = Lib.美食大战老鼠.获取枫叶普通模式句柄()*/
    Case Else
        MessageBox "1P窗口抓取失败"
    End Select
	
    单人获取句柄 = hwnd
	
End Function

Function 获取句柄(way_1p, way_2p)
    Dim hwnd(1)
	
    TracePrint "登陆方式分别为：" & way_1p & ", " & way_2p
    Select Case way_1p
    Case 0
        hwnd(0) = Lib.美食大战老鼠.获取360句柄()
    Case 1
        hwnd(0) = Lib.美食大战老鼠.获取微端句柄()
    Case 2
        hwnd(0) = Lib.美食大战老鼠.获取枫叶内置flash句柄()
        /*Case 3
        hwnd(0) = Lib.美食大战老鼠.获取枫叶普通模式句柄()*/
    Case Else
        MessageBox "1P窗口抓取失败"
    End Select
	
    Select Case way_2p
    Case 0
        hwnd(1) = Lib.美食大战老鼠.获取枫叶内置flash句柄()
        /*Case 1
        hwnd(1) = Lib.美食大战老鼠.获取枫叶普通模式句柄()*/
    Case 1
        hwnd(1) = Lib.美食大战老鼠.获取360句柄()
    Case 2
        hwnd(1) = Lib.美食大战老鼠.获取微端句柄()
    Case Else
        MessageBox "2P窗口抓取失败"
    End Select
	
    获取句柄 = hwnd
End Function

Sub 开房组队
    执行次数 = CInt(Form1.InputBox_tab1_circle_num.Text)
    Dim i
    i=1
    While i <= 执行次数
        Form1.Label_tab1_now_turn_show.Caption = "" & i
        追加信息输出框 ("开始第" & i & "局")
		
        Call 组队从准备到翻牌(1, Form1.OptionBox_tab1_continue_yes.Value, Form1.InputBox_tab1_card_position.Text )
		
        '等待2s进入下一次循环
        追加信息输出框("完成第" & i & "局")
        Delay 2000
        i = i + 1
    Wend
	
    Form1.Label_tab1_now_turn_show.Caption = "0"
    ThreadID_now = 0
    TracePrint ThreadID_now
    追加信息输出框 ("脚本1停止执行")
    If Form1.CheckBox_tab1_finish_remind.Value = 1 Then 
        MessageBox "脚本1停止执行"
    End If
End Sub


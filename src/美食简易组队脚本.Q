Event Form1.LoadOver
    exe_hwnd = Plugin.Window.Find("#32770", "美食简易组队脚本V6.07")
    Call 追加信息输出框("正在尝试关闭小精灵界面上的广告。获取到首级句柄："&exe_hwnd)
    HwndEx = Plugin.Window.FindEx(exe_hwnd, 0, "AfxWnd100s", "HtmlWebFrame")
    HwndEx2 = Plugin.Window.FindEx(HwndEx, 0, "Shell Embedding", "")
    HwndEx3 = Plugin.Window.FindEx(HwndEx2, 0, "Shell DocObject View", "")
    'Call 追加信息输出框("" & HwndEx3)
    Delay 1000
    HwndEx4 = Plugin.Window.FindEx(HwndEx3, 0, "Internet Explorer_Server", "")
    
    If HwndEx4 = 0 Then 
        Call 追加信息输出框("关闭失败！未获取到广告句柄")
    Else 
        Call 追加信息输出框("关闭成功！广告句柄为：" & HwndEx4)
    End If
    
    Delay 1000
    Call Plugin.Window.Hide(HwndEx4)
    Delay 50
    'Call Plugin.Window.Min(exe_hwnd)
    'Delay 5
    'Call Plugin.Window.Restore(exe_hwnd)
    
End Event


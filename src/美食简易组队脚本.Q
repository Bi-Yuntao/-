Sub 一键日常
    If Form1.CheckBox_tab9_job3.Value = 1 Then 
        If Form1.CheckBox_ban_2p.Value = 1 Then 
            MessageBox "不支持单人悬赏"
        Else
            Call 追加信息输出框("开始悬赏三连")
            Call 悬赏三连组队
            Call 追加信息输出框("结束悬赏三连")
        End If
    End If
	
    If Form1.CheckBox_tab9_job4.Value = 1 Then 
        Call 追加信息输出框("开始会长任务")
        Call 会长任务
        Call 追加信息输出框("结束会长任务")
    End If
	
    If Form1.CheckBox_tab9_job5.Value = 1 Then 
        If Form1.CheckBox_ban_2p.Value = 1 Then 
            MessageBox "不支持单人情侣任务"
        Else
            Call 追加信息输出框("开始情侣任务")
            Call 情侣任务
            Call 追加信息输出框("结束情侣任务")
        End If
    End If
	
    If Form1.CheckBox_tab9_job6.Value = 1 Then 
        If Form1.CheckBox_ban_2p.Value = 1 Then 
            Call 追加信息输出框("开始遗迹单人")
            Call 遗迹单人
            Call 追加信息输出框("结束遗迹单人")
        Else
            Call 追加信息输出框("开始遗迹组队")
            Call 遗迹组队
            Call 追加信息输出框("结束遗迹组队")
        End If
    End If
	
    If Form1.CheckBox_tab9_job7.Value = 1 Then 
        Call 追加信息输出框("开始跨服")
        Call 跨服
        Call 追加信息输出框("结束跨服")
    End If
	
    If Form1.CheckBox_tab9_job8.Value = 1 Then 
        If Form1.CheckBox_ban_2p.Value = 1 Then 
            Call 追加信息输出框("开始魔塔单人")
            Call 魔塔单人
            Call 追加信息输出框("结束魔塔单人")
        Else
            Call 追加信息输出框("开始魔塔组队")
            Call 魔塔组队
            Call 追加信息输出框("结束魔塔组队")
        End If
    End If
	
    ThreadID_now = 0
    启用一键日常模式 = False
    TracePrint ThreadID_now
    追加信息输出框("完成一键日常")
    If Form1.CheckBox_tab6_finish_remind.Value = 1 Then 
        MessageBox "完成一键日常"
    End If
End Sub

